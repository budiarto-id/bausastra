<?xml version='1.0' encoding='utf-8'?>
<widget id="pw.tera.bausastra" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bausastra</name>
    <description>
        O.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://bausastra.tera.pw/" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="/android-icon-36x36.png" />
        <icon density="mdpi" src="/android-icon-48x48.png" />
        <icon density="hdpi" src="/android-icon-72x72.png" />
        <icon density="xhdpi" src="/android-icon-96x96.png" />
        <icon density="xxhdpi" src="/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="/android-icon-192x192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="72" src="/android-icon-72x72.png" width="72" />
        <icon height="144" src="/android-icon-144x144.png" width="144" />
    </platform>
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="false" />
    <allow-navigation hap-rule="yes" href="https://bausastra.tera.pw/*" />
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
</widget>
